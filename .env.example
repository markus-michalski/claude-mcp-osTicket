# osTicket API Configuration (REQUIRED)
# API URL and API Key are REQUIRED for all operations
OSTICKET_API_URL=https://your-osticket-instance.example.com
OSTICKET_API_KEY=your-api-key-here
OSTICKET_API_REJECT_UNAUTHORIZED=true

# Default User for Ticket Creation (optional)
# If name/email/topicId are not provided when creating tickets, these defaults are used
OSTICKET_DEFAULT_NAME=Your Name
OSTICKET_DEFAULT_EMAIL=your-email@example.com
OSTICKET_DEFAULT_TOPIC_ID=1

# Logging
LOG_LEVEL=info

# Development
NODE_ENV=production
