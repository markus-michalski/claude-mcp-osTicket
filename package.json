{
  "name": "@claude/mcp-osticket",
  "version": "1.2.2",
  "description": "MCP Server for osTicket integration via SSH tunnel",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release": "npm run build && git push && git push --tags"
  },
  "keywords": [
    "mcp",
    "osticket",
    "claude",
    "model-context-protocol"
  ],
  "author": "Markus",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "dotenv": "^17.2.3",
    "mysql2": "^3.11.5",
    "ssh2": "^1.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/ssh2": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
